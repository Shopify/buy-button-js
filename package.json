{
  "name": "shopify-buy-ui",
  "version": "0.0.0",
  "description": "The JS Buy UI library allows merchants to build Shopify interfaces into any website",
  "main": "build/shopify-buy-ui.js",
  "repository": "git@github.com:Shopify/shopify-buy-ui.git",
  "scripts": {
    "styles": "node ./script/css2js",
    "start": "watchify src/shopify-buy-ui.js -t babelify --outfile build/shopify-buy-ui.js",
    "build": "NODE_PATH=./node_modules rm -rf build && mkdir build && babel ./src --out-dir ./build",
    "pretest": "eslint src/* | browserify test/test.js -t babelify --outfile test/build/test.js",
    "lint": "eslint src/*",
    "test": "testem ci",
    "test_dev": "watchify test/test.js -t babelify --outfile test/build/test.js | testem",
    "postinstall": "NODE_PATH=./node_modules rm -rf build && mkdir build && babel ./src --out-dir ./build"
  },
  "author": "Shopify Inc.",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-shopify": "11.3.1-0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "css": "^2.1.0",
    "eslint": "^2.13.1",
    "eslint-plugin-shopify": "^12.1.0",
    "qunit": "^0.9.1",
    "sinon": "^1.17.4",
    "testem": "^1.9.1",
    "watchify": "^3.7.0",
    "css-to-js-object": "0.0.3"
  },
  "dependencies": {
    "deepmerge": "^0.2.10",
    "hogan.js": "^3.0.2",
    "morphdom": "^1.4.3",
    "shopify-buy": "^0.1.7"
  }
}
