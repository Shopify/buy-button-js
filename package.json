{
  "name": "shopify-buy-ui",
  "version": "0.0.0",
  "description": "The JS Buy UI library allows merchants to build Shopify interfaces into any website",
  "main": "build/shopify-buy-ui.js",
  "repository": "git@github.com:Shopify/shopify-buy-ui.git",
  "scripts": {
    "start": "rm -rf tmp && mkdir tmp && npm run src:watch & npm run styles:watch & npm run serve",
    "serve": "http-server",
    "lint": "eslint src/*",
    "clean": "rm -rf build && mkdir build",
    "styles": "npm run styles-embeds:build && npm run styles-host:build",
    "postinstall": "npm run build",
    "build": "npm run clean && npm run styles && npm run dist:babel && npm run dist:browserify && npm run dist:polyfill && npm run dist:uglify",
    "---------- ci": null,
    "test": "./test-ci",
    "lint-ci": "eslint src --max-warnings=0 -f junit > \"${lint_output_file}\"",
    "testem-ci": "testem ci -R xunit > \"${test_output_file}\"",
    "---------- dist": null,
    "dist:babel": "NODE_PATH=./node_modules babel ./src --out-dir ./build",
    "dist:browserify": "browserify --ignore buffer build/shopify-buy-ui.js --outfile build/shopify-buy-ui.globals.js",
    "dist:polyfill": "NODE_PATH=./node_modules node ./script/polyfill.js shopify-buy-ui ",
    "dist:uglify": "uglifyjs build/shopify-buy-ui.globals.polyfilled.js --output build/shopify-buy-ui.globals.polyfilled.min.js",
    "---------- dev": null,
    "src:watch": "watchify src/shopify-buy-ui.js -t babelify --outfile tmp/shopify-buy-ui.dev.js -v",
    "---------- test": null,
    "test-dev": "npm run test-mocha:watch | testem",
    "test-selenium:run": "NODE_PATH=./node_modules node script/selenium-test.js",
    "test-mocha:build": "npm run styles && browserify test/test.js -t babelify --outfile test/build/test.js",
    "test-mocha:watch": "npm run styles && watchify test/test.js -t babelify --outfile test/build/test.js -v",
    "pretest": "npm run test-mocha:build",
    "---------- css": null,
    "styles:watch": "watch 'npm run styles' src/styles/embeds/sass src/styles/host/sass",
    "styles-embeds:build": "NODE_PATH=./node_modules node ./script/embed-styles",
    "styles-host:build": "NODE_PATH=./node_modules node ./script/host-styles"
  },
  "author": "Shopify Inc.",
  "license": "MIT",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.11.4",
    "babel-core": "6.10.4",
    "babel-plugin-check-es2015-constants": "6.8.0",
    "babel-plugin-transform-es2015-arrow-functions": "6.8.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "6.8.0",
    "babel-plugin-transform-es2015-block-scoping": "6.10.1",
    "babel-plugin-transform-es2015-classes": "6.9.0",
    "babel-plugin-transform-es2015-computed-properties": "6.8.0",
    "babel-plugin-transform-es2015-destructuring": "6.9.0",
    "babel-plugin-transform-es2015-duplicate-keys": "6.8.0",
    "babel-plugin-transform-es2015-function-name": "6.9.0",
    "babel-plugin-transform-es2015-literals": "6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.11.5",
    "babel-plugin-transform-es2015-object-super": "6.8.0",
    "babel-plugin-transform-es2015-parameters": "6.11.4",
    "babel-plugin-transform-es2015-shorthand-properties": "6.8.0",
    "babel-plugin-transform-es2015-spread": "6.8.0",
    "babel-plugin-transform-es2015-template-literals": "6.8.0",
    "babel-plugin-transform-es2015-typeof-symbol": "6.8.0",
    "babelify": "7.3.0",
    "chai": "3.5.0",
    "eslint": "2.13.1",
    "eslint-plugin-shopify": "13.0",
    "fetch-pretender": "1.5.0",
    "global-npm": "0.3.0",
    "http-server": "0.9.0",
    "mocha": "2.5.3",
    "pretender": "1.1.0",
    "sinon": "1.17.5",
    "testem": "1.9.1",
    "watch": "0.19.1",
    "watchify": "3.7.0",
    "wdio-dot-reporter": "0.0.6",
    "wdio-junit-reporter": "0.1.0",
    "wdio-mocha-framework": "^0.4.0",
    "webdriverio": "4.2.8"
  },
  "dependencies": {
    "uglify-js": "2.7.0",
    "browserify": "13.1.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-runtime": "6.11.6",
    "hogan.js": "3.0.2",
    "morphdom": "1.4.6",
    "node-sass": "3.8.0",
    "rollup": "0.34.1",
    "rollup-plugin-commonjs": "3.3.1",
    "rollup-plugin-node-resolve": "1.7.1",
    "shopify-buy": "0.2.3"
  }
}
